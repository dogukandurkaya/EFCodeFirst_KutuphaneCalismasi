@model EF_CodeFirst.Models.Entities.Member
@{
    ViewData["Title"] = "Details";
}

<h1 class="bg-dark text-light">Üye Detayları</h1>

<div class="bg-dark text-light">
    <h4>Üye</h4>
    <h5>Üye ID: </h5>
    <p>@Model.MemberId</p>
    <h5>Üye Ad</h5>
    <p>@Model.MemberName</p>
    <h5>Üye Soyad</h5>
    <p>@Model.MemberSurname</p>
    <h5>Silinme Durumu</h5>
    <p>@Model.IsDeleted</p>
</div>

<a asp-action="Index">Üyeler Sayfasına Dön</a>